
<% layout('/layouts/boilerplate') -%>
<body>
  
    <div style="text-align: center; margin-top: 20px;">
      <h1>Brilliant Student </h1>
      <h5>Mathematic Team Hazaribagh ,jharkhand</h5>
    </div>
   <div class="stuContainer  ">  

    <% for (let student of allStudents) { %>
        
      
        <div class="card stuBox border border-danger" style="width: 18rem;">
            <img src="<%=student.image  %>" class="card-img-top stuimage" alt="student image" height="210px" width="250px">
            <div class="card-body">
               
                <p class="card-text"> <%= student.name%></p>
              </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item">Class: <%= student.class %></li>
              <li class="list-group-item">Batch: <%= student.batch %></li>

            </ul>
           
          </div>
        <br>
    <% } %>
  </div>
</body>

